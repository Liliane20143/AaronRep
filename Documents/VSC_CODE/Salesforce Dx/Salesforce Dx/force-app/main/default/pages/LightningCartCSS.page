<!--  Loading div Style -->
.overlay 
{
    display: none;
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: 0.4;
    -moz-opacity: 0.4;
    -khtml-opacity: 0.4;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=40)";
    filter: alpha(opacity=40);
    background: #000;
    z-index: 1100;
}
.loader
{
    /*background: url('/img/loading32.gif') scroll no-repeat 0 0;*/
    background: url({!URLFOR($Resource.NE__Cart_resource, 'img/loading_transparent.gif')}) no-repeat;
    width: 70px;
    height: 70px;
    position: absolute;
    left: 50%;
    z-index: 1100;
}
.overlay-promoitem 
{
    display: none;
    <!-- left: 0;
    top: 0; -->
    width: 100%;
    height: 100%;
    <!-- opacity: 0.8;
    -moz-opacity: 0.8;
    -khtml-opacity: 0.8;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
    filter: alpha(opacity=80);
    background: #000; -->
    z-index: 1100;
}
.loader-promoitem 
{
    background: url('/img/loading32.gif') scroll no-repeat 0 0;
    width: 32px;
    height: 32px;
    margin 10px;
    left: 50%;
    z-index: 1100;
}
.loading-complex
{
    background: url('/img/loading.gif') scroll no-repeat 0 0;
    width: 16px;
    height: 16px;
}
MYcomplexConf, MYcomplexConf2
{
    width: 100%;
    height: 200px; 
}

<!-- General -->
.debugPanelClass
{
    width: 70%;
    display: inline-block;
}
.debugPanelClass .message
{
    margin: 4px 35px 4px 0px;
}
.bodyDiv
{
    overflow: visible;   
}
.hideArea
{
    display:none !important;
}
.noSidebarCell
{
    padding-top: 0px !important;   
}
th
{
    text-align: center !important;
}
#tsid, #userNav, #MoreTabs_List
{
    z-index: 1000;
}
.pointer
{
    cursor: pointer;
}
button[disabled]
{
    color: #d8dde6 !important;
    background: #ffffff !important;
}
button[disabled]:hover 
{
    background: transparent !important;
}
.errorMsg
{
    line-height: 1em;
}
.rich-table-thead
{
    border-bottom: 0px solid #E0E3E5 !important;
}

<!-- Layout -->
div[id$=":header"]
{
    padding: 20px 0px;
    overflow: auto;
    min-height: 32px;
    margin-right: 320px;
}
div[id$=":section-left"]
{
    float: left;
    width: 200px;
}
div[id$=":section-right"]
{
    margin-left: 220px;    
}

<!-- Header -->
div[id$=":header"] table
{
    width: 100%;
}
#commModel-navBar
{
    margin: 0;
    padding: 0;
}
#commModel-navBar li
{
    font-size: 1.5em;
    font-weight: bold;
    list-style: none outside none;
    float: left;
    width: 31%;
    line-height: 25px;
    margin: 0px 10px;
}
#commModel-navBar a
{
    text-decoration: none;
    font-size: 15px;
    font-weight: normal;
    text-transform: uppercase;
}
#commModel-navBar a:hover
{
    text-decoration: underline;
}
.cartCell
{
    width: 80px;
    text-align: right;
    border-radius: 0px 5px 5px 0px;
}

<!-- Section left -->
.sectionLeft-collapserHandle
{
    position: relative;
    top: 200px;
    left: 15px;
    float: left;
    cursor: pointer;
}
    
<!-- Box -->
.box
{
    background: none repeat scroll 0 0 #FFFFFF;
    border-radius: 5px;
    margin-bottom: 20px;
}
.box-complexmessages
{
    background: none repeat scroll 0 0 #FFFFFF;
    border-radius: 5px;
}
.box-header
{
    background-color: #ffffff;
    border: 1px solid #d8dde6;
    border-radius: 0;
    color: #ffffff;
    height: 30px;
    line-height: 30px;
    border-width: 1px 1px 0px 1px;
}
.box-header a
{
}
.box-header-arrow
{
    cursor: pointer;
    margin-left: 5px;
    width: 8px;
    height: 8px;
}
.box-header-arrow:not(#hierarchyArrow)
{
    display: none;
}
.box-body-categories-hierarchy
{
    display:none;
}
.box-header .opened
{
    background: url({!URLFOR($Resource.NE__Cart_resource, 'img/arrow_black_down_8.png')}) no-repeat;
}
.box-header .closed
{
    background: url({!URLFOR($Resource.NE__Cart_resource, 'img/arrow_black_right_8.png')}) no-repeat;
}
[id$=":section-left"] > div:first-of-type > .box-header 
{
    display: none;
}
[id$=":section-left"] > div:first-of-type > .box-body 
{
    border-top: solid 1px #E0E3E5;
}
.box-header-title 
{
    margin-left: 5px;
    font-weight: normal;
    font-size: 1.2em;
    text-transform: uppercase;
    color: #333;
}
a.box-header-title 
{
    color: #0070d2;
    text-decoration: none;
}
.box-subHeader
{
    background-color: #ffffff;
    border-width: 0px 1px 0px 1px;
    border-style: solid;
    border-color: #E0E3E5;
}
.box-body
{
    background-color: #ffffff;
    padding-bottom: 5px;
    border-width: 0px 1px 1px 1px;
    border-style: solid;
    border-color: #E0E3E5;
}
.box-body-categories
{
}

/******** Category ********/
.category-info 
{
    font-weight: bold;
    color: #1797C0 !important;
}
.category-info-grid
{
    width: 500px;
}
.category-info table td 
{
    color: #1797C0 !important;
}
.black-box 
{
    width: 150px;
    height: 150px;
}
.hideRow
{
    display:none;
}
    
<!-- TREE STYLESHEET -->
.jstree-classic.jstree-focused 
{
    background-color: #ffffff !important;
}
.item-complex > a > .jstree-icon
{
    background-image: url({!URLFOR($Resource.NE__Cart_resource, 'img/product-complex_16x16.png')}) !important;
    background-position: 0px 0px !important;
    background-repeat: none;
}
.item-simple > a > .jstree-icon
{
    background-image: url({!URLFOR($Resource.NE__Cart_resource, 'img/product-simple_16x16.png')}) !important;
    background-position: 0px 0px !important;
    background-repeat: none;
}
.item-selected > a
{
    background: #CCE7EE !important;
}

<!-- ET 13-01-15 manage tree font size -->
.jstree a 
{
    display: inline-block;
    line-height: 16px;
    height: 16px;
    color: black;
    white-space: nowrap;
    text-decoration: none;
    padding: 1px 2px;
    margin: 0;
    font-size: 12px;
}
    
<!-- Messages -->
.box-body-messages
{
    height: 300px;
    overflow: auto;
}
.msg
{
    padding: 5px;
