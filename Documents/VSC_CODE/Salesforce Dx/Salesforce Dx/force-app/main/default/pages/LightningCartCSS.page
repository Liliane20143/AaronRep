<!-- DROP DOWN MENU -->
ul.dropdown, ul.dropdown li, ul.dropdown ul
{
    list-style: none;
    margin: 0;
    padding: 0;
}
ul.dropdown
{
    position: relative;
    z-index: 597;
}
ul.dropdown li
{
    background-color: #ffffff;
    float: left;
    min-height: 1px;
    vertical-align: middle;
}
ul.dropdown li.hover, ul.dropdown li:hover
{
    position: relative;
    z-index: 599;
    cursor: default;
}
ul.dropdown ul
{
    display: none;
    position: absolute;
    top: 100%;
    left: 0;
    z-index: 598;
    width: 100%;
}
ul.dropdown ul li
{
    float: none;
    background-color: #FFFFFF;
}
ul.dropdown ul ul
{
}
ul.dropdown li:hover > ul
{
    display: block;
}

/* Vertical Left-to-Right Direction CSS Drop-Down Menu Component */
ul.dropdown-vertical
{
    width: 198px;
}
ul.dropdown-vertical ul
{
    top: 0px;
    left: 100%;
}
ul.dropdown-vertical li
{
    float: none;
}

/* -- Base drop-down styling -- */
ul.dropdown ul
{
    padding-left: 15px;     
}
ul.dropdown ul li
{
    background-color: #FFFFFF;      
    border-left: 1px solid #CCCCCC;
    border-right: 1px solid #CCCCCC;
}
ul.dropdown ul li:first-child
{
    border-top: 1px solid #CCCCCC;
}
ul.dropdown ul li:last-child
{
    border-bottom: 1px solid #CCCCCC;
}
ul.dropdown ul:after 
{
    background: url("{!URLFOR($Resource.NE__Cart_resource, 'img/arrow_grey_left.png')}") no-repeat scroll 0 0 transparent;
    content: "";
    height: 20px;
    left: 1px;
    position: absolute;
    top: 7px;
    width: 15px;
    z-index: 9999;
}
ul.dropdown li
{
    padding: 7px 10px;
    color: #000;
}
ul.dropdown li.hover, ul.dropdown li:hover, ul.dropdown li.on
{
    color: #000;
}
ul.dropdown a
{
    padding-left: 20px;
}
ul.dropdown a:link,
ul.dropdown a:visited   { color: #000; text-decoration: none; }
ul.dropdown a:hover     { color: #000; text-decoration: underline;}
ul.dropdown a:active    { color: #015BA7; }

/* -- level mark -- */
ul.dropdown ul
{
    width: 198px;
    margin-top: 0px;
}
ul.dropdown ul li
{
    font-weight: normal;
}

/* -- Supporting class `dir` -- */
ul.dropdown *.dir
{
    padding-right: 15px;
    background-image: url({!URLFOR($Resource.NE__Cart_resource, 'img/arrow_down.png')});
    background-position: 100% 50%;
    background-repeat: no-repeat;
}

/* -- Components override -- */
ul.dropdown-horizontal ul *.dir
{
    padding-right: 15px;
    background-image: url({!URLFOR($Resource.NE__Cart_resource, 'img/arrow_right.png')});
    background-position: 100% 50%;
    background-repeat: no-repeat;
}
ul.dropdown-upward *.dir
{
    background-image: url({!URLFOR($Resource.NE__Cart_resource, 'img/arrow_up.png')});
}
ul.dropdown-vertical *.dir, ul.dropdown-upward ul *.dir
{
    background-image: url({!URLFOR($Resource.NE__Cart_resource, 'img/arrow_right.png')});
}
ul.dropdown-vertical-rtl *.dir, ul.dropdown-horizontal > li.dropdown-vertical-rtl ul *.dir
{
    padding-right: 10px;
    padding-left: 15px;
    background-image: url({!URLFOR($Resource.NE__Cart_resource, 'img/arrow_left.png')});
    background-position: 2px 50%;
}
    
