<!-- Search Bar -->
.searchBar
{
    border: none;
    background-color: #ffffff;
    overflow: auto;
    color: #333435;
    padding-bottom: 10px;
}
.searchBar-header
{
    margin-top: 5px;
    padding-left: 47%;
}
.searchBar-leftPanel, .searchBar-rightPanel
{
    width: 49.5%;
    float: left;
}
.searchBar-leftPanel table, .searchBar-rightPanel table
{
    margin: 0px auto;
} 
.searchBar-collapser
{
    display: none;
    /*
    border: 1px solid #d8dde6;
    height: 25px;
    width: 140px;
    margin: auto;
    text-align: center;
    font-weight: bold;
    border-top-width: 0px;
    */
}
.searchBar-collapser-complexcart
