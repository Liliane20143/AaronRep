<!-- Root Category Link -->
.rootCategoryWrapper
{
    border-bottom: solid 1px #E0E3E5;
    height: 30px;
    line-height: 30px;
    text-decoration: none;
    background-color: #ffffff;
    border-radius: 0;
    font-weight: normal;
    text-transform: uppercase;
