    margin-left: 2px;
    width: 165px;
    padding-top: 2px;
}
    
<!-- Quantity input -->
.quantity-input 
{
    border: 1px solid #d8dde6;
    border-style: solid;
    font-size: 82.5%;
    height: 13px;
    width: 40px;
    text-align: right;
    padding: 2px;
}
  
<!-- catalogItems-table -->
.catalogItems-table
{
    width: 100%;
    border-collapse: collapse;
    line-height: 2.5em;    
}
.catalogItems-table>tbody>tr>td:last-child
{
    border: none;
}
.catalogItems-table th
{
    padding: 0px 5px;
    border: none;
    background-color: #ffffff;
    border-bottom: 1px solid #E0E3E5;
    border-right: none;
    border-left: none;
    font-weight: normal;
    text-transform: uppercase;
    font-size: 11px;    
}
.catalogItems-table th > div
{
    display: none;
}
.cart-content-box-body > .catalogItems-table th
{
    display: none;
}
.catalogItems-table td
{
    background-color: #FFFFFF;
    padding-left: 5px;
    border-bottom: 1px solid #E0E3E5;
}
.cart-content-box-body > .catalogItems-table tr 
{
    border-top: 1px solid #E0E3E5;
    border-bottom: 1px solid #E0E3E5;
}
.catalogItems-table tr 
{
    border-bottom: 1px solid #E0E3E5;
}

<!-- complex-catalogItems-table -->
.catalogItems-table-complex
{
    width: 100%;
    border-collapse: collapse;
}
.catalogItems-table-complex th
{
    padding: 0px 5px;
    background-color: #F2F3F3;
    display:none;
    
}
.catalogItems-table-complex td
{
    padding-left: 0px;
}
.catalogItems-table-complex tr
{
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
}
.catalogItems-table-complex > tbody > tr > td > .img-compare 
{
    margin-right: 10px;
}
 .main-content-box-header-selectedItems + .catalogItems-table > tbody > tr:first-of-type 
{
    display: none;
}   

<!-- CART -->
div#cart
{
    position: absolute;
    top 0px; left: 10px; right: 10px;
    height: 0px;
    z-index: 997;
}
#cart-handle
{
    background-color: #4F6A92;
    box-shadow: none !important;
    width: 340px;
    height: 84px;
    position: initial;
    top: 0px; right: 0px;
    float: right;
    z-index:998;
    cursor:pointer;
    min-width: 280px;
}
#cart-handle > table 
{
    margin-top: 9px;
}
.cart-handle-complex-config
{
    right: 240px !important;
}
.cart-handle-complex
{
    min-width: 210px !important;
}
.complex-handle
{
    top: 0px; right: 0px;
    float: right;
    -webkit-border-bottom-right-radius: 5px;
    -webkit-border-bottom-left-radius: 5px;
    -moz-border-radius-bottomright: 5px;
    -moz-border-radius-bottomleft: 5px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    z-index:998;
    background: #1797C0;   
}
div[id$=":tree"] 
{
    height: 304px;
    padding-top: 10px;
    overflow: auto;
}
.complex-handle table td
{
    color: #FFFFFF;
}
#cart-handle:hover
{
    background: #A7B8D1;
    background: -moz-linear-gradient(top,  #A7B8D10%, #7F8DE1 34%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#A7B8D1), color-stop(34%,#7F8DE1));
    background: -webkit-linear-gradient(top,  #A7B8D10%,#7F8DE1 34%);
    background: -o-linear-gradient(top,  #A7B8D10%,#7F8DE1 34%);
    background: -ms-linear-gradient(top,  #A7B8D10%,#7F8DE1 34%);
    background: linear-gradient(to bottom,  #A7B8D10%,#7F8DE1 34%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#A7B8D1', endColorstr='#7F8DE1',GradientType=0 );
}
#cart-handle table td
{
    color: #FFFFFF;
}
.cart-handle-label
{
    font-weight: normal;
    text-transform: uppercase;
    margin-left: 1em;
}
.cart-handle-value
{
    font-weight: normal;
    margin-left: 1em;
    font-size: 1.2em;
    line-height: 0.6em;
}
.statusValid
{
    color: #00FF00;
}
.statusNotValid
{
    color: #FFCC33;
}
#cart-slide
{
    background: #EEF1F6;
    padding: 0px 15px 15px 15px;
    z-index:999;
    margin-left: 220px;
}
#cart-slide .cart-command
{
    padding-top: 20px;
    padding-bottom: 25px;
    color: #FFFFFF;
}
#cart-slide .cart-content
{
    padding: 15px;
    border: 1px solid #E0E3E5;
    background-color: #FFFFFF;
    overflow-y:auto;
    max-height:500px;
} 
#cart-slide .cart-content-box
{
    margin-bottom: 40px;
}
#cart-slide .cart-content-box-header
{
    display: none;
    /*height: 26px;
    line-height: 26px;
    background-color: #545355;
    margin: 0px auto;
    font-size: 0.9em;
    font-weight: bold;
    color: #FFFFFF;
    padding-left: 10px;
    border-radius: 2px 2px 2px 2px;*/
}
#cart-slide .cart-content-box-body
{
    padding: 10px;
}
    
 <!-- OI 3.8.8 --> 
.contentCart
{
    overflow-y: auto;
    max-height: 500px;
}
    
