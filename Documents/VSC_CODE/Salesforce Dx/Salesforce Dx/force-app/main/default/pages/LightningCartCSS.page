<!-- NO-ITEMS -->
.no-items{
    text-align: center;
}
    
<!-- INFO-POPUP -->
.info-popup
{
    position: relative;
    top: 8px;
    left: -4px;
    display: none;
}
.info-popup-arrow
{
    position: absolute;
    top: -9px; left: 14px;
    width: 25px;
    height: 25px;
    background: url({!URLFOR($Resource.NE__Cart_resource, 'img/dialogbox_arrows.png')}) no-repeat scroll left top transparent;
    background-position: -2px -242px;
    z-index: 2;
}
.info-popup-box
{
    background: #FFFFFF;
    border: 1px solid #E0E3E5;
    position: absolute;
    top: 3px; left: -22px;
    
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    padding: 10px;
    width: 900px;
    max-height: 400px;
    overflow-y: auto;
    z-index: 1;
}
.info-popup-box-header
