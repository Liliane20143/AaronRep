<apex:page standardController="ProcessInstanceWorkitem" 
			sidebar="true" 
			showHeader="true"
           lightningStyleSheets="true">
    <apex:slds />
    <apex:form >
        <div class="slds-notify slds-notify_alert slds-theme_alert-texture slds-theme_error" role="alert">
            <span class="slds-assistive-text">{!$Label.OB_CustomErrorLabelTech}</span>
            <span class="slds-icon_container slds-icon-utility-error slds-m-right_x-small" title="Description of icon when needed"/>
            <h2> {!$Label.OB_ImpossibleToProceedLabel} </h2> 
            <div class="slds-notify__close">
                <button class="slds-button slds-button_icon slds-button_icon-inverse" title="Close">
                    <span class="slds-assistive-text">{!$Label.Close}</span>
                </button>
            </div>
    	</div>
        <div/>
           <!--  <div class="slds"> -->
           
           <div class="slds-grid slds-grid_vertical">
			  <div class="slds-col" style="height: 2rem;">
			  	<span></span>
			  </div>
			  <div class="slds-col">
			    <span></span>
			  </div>
			  <div class="slds-col">
			    <span>
			    	<apex:commandButton action="{!Cancel}" value="{!$Label.OB_ButtonBack}" styleClass="slds-button slds-button--brand"/>
			    </span>
			  </div>
			</div>

        
	</apex:form>

</apex:page>