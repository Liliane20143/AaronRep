  	<div class="slds-size--1-of-1 slds-grid slds-wrap">
  		<aura:if isTrue="{!v.objectDataMap.isCommunityUser == false}">
	  		<div class="slds-size--1-of-2 ">
				<div class="slds-p-around--medium slds-form-element" >
