				<!-- Doris D. ****** 13/03/2019 ***** START-->
				<lightning:helptext content= "{!$Label.c.OB_PreliminaryVerificationTooltip}" /> 
				<!-- Doris D. ****** 13/03/2019 ***** END-->
				<lightning:input label=""
