				</lightning:layoutItem>
			</lightning:layout>

			<aura:if isTrue="{!v.showTEmodal}">
				<!--NEXI-59 Joanna Mielczarek <joanna.mielczarek@accenture.com>, 22.06.2019 START added fiscalCodes-->
				<c:OB_LCP006_NewOrModifyContactModal accountId="{!v.objectDataMap.acc.id}"
