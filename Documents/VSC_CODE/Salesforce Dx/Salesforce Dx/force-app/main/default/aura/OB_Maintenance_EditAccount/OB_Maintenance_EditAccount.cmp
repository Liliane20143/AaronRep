	<lightning:accordion allowMultipleSectionsOpen="true">
		<lightning:accordionSection name="ActualOwnersData" label="{!$Label.c.OB_ActualOwnerData}">
			<!--NEXI-58 Adrian Dlugolecki<adrian.dlugolecki@accenture.com>, 30/05/2019 Start-->
			<aura:if isTrue="{! v.isLoaded }">
				<aura:if isTrue="{! v.refreshOwnersTable }">
					<c:OB_LCP006_ContactDataTable tableData="{!v.tableData}"
												  accountId="{!v.objectDataMap.acc.Id}"
												  fiscalCodes="{! v.fiscalCodes }"
												  isTEDeletePossible="{!v.isTEDeletePossible}"
												  isTEInsertPossible="{!v.isTEInsertPossible}"

					/>
				</aura:if>
			</aura:if>

			<lightning:layout>
				<lightning:layoutItem size="3">
				</lightning:layoutItem>
				<lightning:layoutItem size="6">
					<lightning:layout >
						<lightning:layoutItem padding="horizontal-small" >
							<lightning:button label="{!$Label.c.OB_NewEffectiveOwner}" variant="brand" onclick="{!c.openOwnerSectionModal}" disabled="{! v.hasLogRequest || !v.isTEInsertPossible }"/>
						</lightning:layoutItem>
						<lightning:layoutItem padding="horizontal-small">
							<lightning:button label="{!$Label.c.OB_ConfirmChangeOfOwners}" variant="brand" disabled="{!!v.areChangesOnOwnerSection}" onclick="{!c.confirmOwnerChanges}"/>
						</lightning:layoutItem>
						<lightning:layoutItem padding="horizontal-small">
							<lightning:button label="{!$Label.c.OB_CancelChangeOfOwners}" disabled="{!!v.areChangesOnOwnerSection}" onclick="{!c.cancelOwnerChanges}"/>
						</lightning:layoutItem>
