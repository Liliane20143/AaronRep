		    <lightning:buttonGroup class="slds-align_absolute-center" >
				<aura:if isTrue = "{!!v.listEmpty}">
					<div>
						<lightning:button class ="draft" variant="neutral" aura:id="SaveAsDraft"  label="{!$Label.c.OB_MAINTENANCE_SAVEASDRAFT}" title="{!$Label.c.OB_MAINTENANCE_SAVEASDRAFT}" onclick="{! c.saveAsDraft }" />
					</div>
					<!-- micol.ferrari 24/01/2019 - REMOVED class="close" -->
					<div>
						<lightning:button variant="brand" aura:id="closeModal"  label="{!$Label.c.OB_MAINTENANCE_CONFIRM}" title="{!$Label.c.OB_MAINTENANCE_CONFIRM}" onclick="{! c.closeModal }" />
					</div>
					<aura:set attribute="else">
						<lightning:button variant="brand" aura:id="closeModal"  label="{!$Label.c.OB_MAINTENANCE_BUTTON_CLOSEBUTTON}" title="{!$Label.c.OB_MAINTENANCE_BUTTON_CLOSEBUTTON}" onclick="{! c.closeModalNoDocs }" />
